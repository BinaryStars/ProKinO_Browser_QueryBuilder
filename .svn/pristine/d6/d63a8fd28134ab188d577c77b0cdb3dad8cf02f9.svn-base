package edu.uga.prokino.browser;
import java.util.Comparator;


/**
 * @author Krys J. Kochut
 *
 * * This class is a comparator of Mutations.
 */
public class MutationComparator implements
        Comparator<Mutation>
{
    public int compare(Mutation m1, Mutation m2)
    {
        return m1.getStartPosition() - m2.getStartPosition();
    }
}
